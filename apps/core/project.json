{
  "sourceRoot": "apps/core",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o='./bin/tensoremr' ./main.go",
        "cwd": "apps/core"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go test -v ./...",
        "cwd": "apps/core"
      }
    },
    "docker": {
      "executor": "@nx-tools/nx-docker:build",
      "options": {
        "metadata": {
          "images": ["ghcr.io/tensorsystems/tensoremr/apps/core"],
          "load": true,
          "push": true,
          "tags": ["latest"]
        }
      }
    }
  },
  "tags": []
}
