{
    "sourceRoot": "apps/loinc-import",
    "projectType": "application",
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "go build -o='./bin/tensoremr' ./main.go",
          "cwd": "apps/loinc-import"
        }
      },
      "serve": {
        "executor": "nx:run-commands",
        "options": {
          "command": "go run ./main.go",
          "cwd": "apps/loinc-import"
        }
      },
      "docker": {
        "executor": "@nx-tools/nx-docker:build",
        "options": {
          "metadata": {
            "images": ["ghcr.io/tensorsystems/tensoremr/apps/loinc-import"],
            "load": true,
            "push": true,
            "tags": ["latest"]
          }
        }
      }
    },
    "tags": []
  }
  