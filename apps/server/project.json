{
  "root": "apps/server",
  "sourceRoot": "apps/server",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build -o='./bin/tensoremr' ./main.go ./bin/tensoremr",
        "cwd": "apps/server"
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go run ./main.go",
        "cwd": "apps/server"
      }
    }
  },
  "tags": []
}
